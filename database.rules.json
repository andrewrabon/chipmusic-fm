{
  "rules": {
    "music": {
      "songs": {
        "$songId": {
          "listenCount": {
            ".write": true
          }
        },
        ".read": true
      }
    },
    "userSettings": {
      "$userId": {
        ".read": "auth != null && $userId == auth.uid",
        ".write": "auth != null && $userId == auth.uid"
      }
    }
  }
}
